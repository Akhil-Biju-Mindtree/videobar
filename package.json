{
  "name": "Bose-Work-Configuration",
  "version": "0.12.0",
  "description": "Bose Work Configuration",
  "homepage": "https://github.com",
  "author": {
    "name": "Bose Corporation"
  },
  "keywords": [
    "angular",
    "angular 8",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "electron/main.js",
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "moduleNameMapper": {
      "@shared/(.*)": "<rootDir>/src/app/shared/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@environment/(.*)": "<rootDir>/src/environments/$1",
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@camera-view/(.*)": "<rootDir>/src/app/camera-view/$1",
      "@providers/(.*)": "<rootDir>/src/app/providers/$1"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/e2e"
    ]
  },
  "scripts": {
    "preinstall": "node shared/configurations/preinstall-script.js",
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "electron:rebuild": "electron-rebuild --force",
    "ng": "ng",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "start:no-dist": "npm run postinstall:electron && ng build && npm-run-all -p ng:serve electron:serve",
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "copy-config:mac": "cp shared/configurations/configuration.prod.js shared/configurations/configuration.js",
    "copy-config:windows": "cp ./shared/configurations/configuration.prod.js shared/configurations/configuration.js",
    "copy-mainwindow:ts": "cp ./electron/shared/windows/mainWindow-dev-tools.js electron/shared/windows/mainWindow.js",
    "copy-builder:json": "cp ./electron-builder-dev.json electron-builder.json",
    "ng:serve": "ng serve --aot",
    "ng:serve:web": "npm run postinstall:web && npm run ng:serve -- -c web-dev -o",
    "ng:build:web-prod": "npm run postinstall:web && ng build --configuration web-prod",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve ",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && npm run copy-config:windows && electron-builder --windows ",
    "electron:windows-devtools": "npm run build:prod && npm run copy-config:windows && npm run copy-mainwindow:ts && npm run copy-builder:json && electron-builder --windows",
    "electron:mac": "npm run build:prod && npm run copy-config:mac && electron-builder build --mac",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e": "npm run build:prod && mocha --timeout 300000 --require ts-node/register e2e/**/*.spec.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "debug:server": "npm run ng:serve && npm run electron:serve-tsc ",
    "server": "npm rebuild && node server.js",
    "check:all": "ng build --prod && ng lint && npm run electron:windows",
    "compodoc": "npx compodoc -p src/tsconfig.app.json",
    "lint:all": "tslint --project .",
    "ngbuild:prod": "ng build --prod --aot",
    "build:server": "tsc -p server/firmware/tsconfig.json",
    "bundle:web": "cd dist/ && bestzip ../webUI.zip *",
    "build:web-prod:server": "npm run ng:build:web-prod && npm run bundle:web",
    "set-version": "node set-version.js"
  },
  "husky": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular/cli": "8.3.29",
    "@angular/common": "8.2.14",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@compodoc/compodoc": "^1.1.9",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/jest": "^24.0.15",
    "@types/node": "8.9.4",
    "@types/progress-stream": "^2.0.0",
    "codelyzer": "^5.0.1",
    "conventional-changelog-cli": "2.0.11",
    "core-js": "2.6.1",
    "electron": "9.2.1",
    "electron-builder": "^22.7.0",
    "electron-rebuild": "^1.8.5",
    "electron-reload": "1.3.0",
    "fabric": "^3.4.0",
    "husky": "^3.0.4",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "jest": "24.9.0",
    "jest-cli": "^25.2.7",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-preset-angular": "^8.1.0",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-jasmine": "^3.1.1",
    "karma-typescript": "^5.0.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "4.1.5",
    "spectron": "^11.0.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-sonarts": "^1.9.0",
    "typescript": "3.5.3",
    "wait-on": "3.2.0",
    "webdriver-manager": "12.1.0",
    "zone.js": "~0.9.1"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/material": "^8.2.3",
    "@ng-idle/core": "^8.0.0-beta.4",
    "@ngrx/effects": "7.4.0",
    "@ngrx/router-store": "7.4.0",
    "@ngrx/store": "7.4.0",
    "@ngrx/store-devtools": "7.4.0",
    "ajv": "^6.10.0",
    "bestzip": "^2.1.7",
    "body-parser": "^1.19.0",
    "drivelist": "^8.0.7",
    "electron-log": "^3.0.6",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.19",
    "ngx-cookie-service": "^2.2.0",
    "ngx-file-drop": "^8.0.8",
    "ngx-logger": "^3.4.5",
    "node-hid": "1.3.0",
    "p5": "^0.9.0",
    "progress-stream": "^2.0.0",
    "rxjs": "6.6.2",
    "ts-md5": "^1.2.6",
    "usb-detection": "4.10.0",
    "uuid": "^3.3.2",
    "ws": "^7.0.1"
  },
  "engines": {
    "node": ">=10.9.0"
  },
  "browserslist": [
    "defaults"
  ]
}
